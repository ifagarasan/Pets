<Window x:Class="SyncMaester.SyncManager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SyncMaester"
        mc:Ignorable="d"
        Title="SyncMaester - Sync Manager" Height="627" Width="533">
    <Grid>
        <ListBox x:Name="syncPairs" HorizontalAlignment="Left" Margin="10,52,0,129" Width="505"/>
        <Button x:Name="add" Content="Add" HorizontalAlignment="Left" Margin="10,472,0,0" VerticalAlignment="Top" Width="138" Height="53" Click="add_Click"/>
        <Button x:Name="save" Content="Save" HorizontalAlignment="Left" Margin="377,533,0,0" VerticalAlignment="Top" Width="138" Height="53" Click="save_Click"/>

    </Grid>
</Window>
